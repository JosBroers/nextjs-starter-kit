{
	"name": "nextjs-skeleton",
	"version": "1.0.9-canary.1",
	"author": "Jos Broers",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel --continue",
		"clean": "node scripts/clean/clean.mjs",
		"lint": "turbo run lint",
		"prepare": "husky install",
		"make_public": "json -I -f package.json -e 'this.private=false'",
		"upgrade_all": "yarn upgrade-interactive --latest",
		"copy_readme": "copy README.md scripts/create-project/",
		"lerna": "npx lerna version --amend"
	},
	"workspaces": [
		"apps/*",
		"packages/*",
		"scripts/*"
	],
	"devDependencies": {
		"config": "1.0.9-canary.1",
		"copy": "^0.3.2",
		"eslint": "^8.15.0",
		"husky": "^8.0.1",
		"json": "^11.0.0",
		"lerna": "^4.0.0",
		"prettier": "^2.6.2",
		"stylelint": "^14.8.3",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^3.0.0",
		"turbo": "1.2.9"
	},
	"volta": {
		"node": "14.18.2",
		"yarn": "1.22.17"
	},
	"engines": {
		"node": ">=14.18.0"
	},
	"packageManager": "yarn@1.22.17"
}
