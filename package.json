{
	"name": "@SirRedDAB/nextjs-skeleton",
	"version": "0.5.2-canary.3",
	"author": "Jos Broers",
	"license": "MIT",
	"private": true,
	"repository": {
		"url": "https://github.com/SirRedDAB/nextjs-skeleton",
		"type": "git"
	},
	"publishConfig": {
		"@SirRedDAB:registry": "https://npm.pkg.github.com"
	},
	"bin": "./scripts/cli.js",
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --no-cache --parallel --continue",
		"clean": "rm -rf node_modules && rm yarn.lock",
		"lint": "turbo run lint",
		"prepare": "husky install",
		"upgrade_all": "yarn upgrade-interactive --latest",
		"make_public": "json -I -f package.json -e 'this.private=false'"
	},
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"devDependencies": {
		"eslint": "^8.5.0",
		"husky": "^7.0.4",
		"json": "^11.0.0",
		"lerna": "^4.0.0",
		"prettier": "^2.5.1",
		"turbo": "1.0.19"
	},
	"volta": {
		"node": "14.18.2",
		"yarn": "1.22.17"
	},
	"engines": {
		"node": ">=14.18.0"
	},
	"turbo": {
		"baseBranch": "origin/main",
		"pipeline": {
			"build": {
				"dependsOn": [
					"^build"
				],
				"outputs": [
					".next/**"
				]
			},
			"lint": {
				"outputs": []
			},
			"dev": {
				"cache": false
			}
		}
	}
}
