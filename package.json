{
	"name": "create-nextjs-skeleton",
	"version": "1.1.5",
	"license": "MIT",
	"private": true,
	"main": "scripts/create.mjs",
	"author": "Jos Broers",
	"description": "A simple and highly customizable skeleton build with Turborepo and Next.js. Featuring ESLint, Husky, Prettier, Sass, TypeScript and much more!",
	"homepage": "https://nextjs-skeleton.jobrodo.nl/",
	"bugs": {
		"url": "https://github.com/josbroers/nextjs-skeleton/issues"
	},
	"keywords": [
		"nextjs",
		"skeleton"
	],
	"repository": {
		"url": "git+https://github.com/jos-broers/nextjs-skeleton.git",
		"type": "git"
	},
	"publishConfig": {
		"access": "public"
	},
	"bin": {
		"create-nextjs-skeleton": "scripts/create.mjs"
	},
	"volta": {
		"node": "16.15.0",
		"yarn": "1.22.17"
	},
	"engines": {
		"node": ">=16.15.0"
	},
	"scripts": {
		"dev": "next dev --turbo",
		"build": "next build",
		"start": "next start",
		"lint": "next lint && stylelint 'scss/**/*.scss'",
		"clean": "node scripts/clean.mjs",
		"prepare": "husky install",
		"make_public": "json -I -f package.json -e 'this.private=false'",
		"update": "npx npm-check -u",
		"copy_readme": "copy README.md scripts/create-project/",
		"lerna": "npx lerna version --force-publish"
	},
	"dependencies": {
		"@next/font": "^13.0.3",
		"@svgr/webpack": "^6.2.1",
		"autoprefixer": "^10.4.7",
		"modern-normalize": "^1.1.0",
		"next": "^13.0.3",
		"postcss": "^8.4.14",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"sass": "^1.51.0",
		"sharp": "^0.31.0"
	},
	"devDependencies": {
		"@types/node": "^18.0.3",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"arg": "^5.0.1",
		"chalk": "^5.0.1",
		"copy": "^0.3.2",
		"edit-json-file": "^1.7.0",
		"eslint": "^8.27.0",
		"eslint-config-next": "^13.0.3",
		"eslint-config-prettier": "^8.5.0",
		"glob": "^8.0.3",
		"husky": "^8.0.1",
		"inquirer": "^9.0.0",
		"json": "^11.0.0",
		"lerna": "^6.0.3",
		"listr": "^0.14.3",
		"prettier": "^2.6.2",
		"stylelint": "^14.8.3",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^6.1.0",
		"turbo": "^1.6.3",
		"typescript": "^4.6.4"
	}
}
